mkdir -p usr/share/jfreechart-1.5.5
cp ${THOME}/tarballs/jfreechart-1.5.5.jar usr/share/jfreechart-1.5.5

mkdir -p usr/share/jfreechart-1.5.6
cp ${THOME}/tarballs/jfreechart-1.5.6.jar usr/share/jfreechart-1.5.6

cd usr/share/jfreechart-1.5.6
wget https://www.gnu.org/licenses/old-licenses/lgpl-2.1.txt
ln lgpl-2.1.txt ../jfreechart-1.5.5/lgpl-2.1.txt
