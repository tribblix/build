#!/bin/sh
#
# put a wrapper in the regular path
#
mkdir -p usr/bin
cd usr/bin
cat > commix <<EOF
#!/bin/sh
cd /usr/versions/commix
exec /usr/versions/python-3.7/bin/python ./commix.py "\$@"
EOF
chmod a+x commix
#
# clean up some junk
#
cd ../versions/commix
rm -f .gitignore .travis.yml
rm -fr .github
