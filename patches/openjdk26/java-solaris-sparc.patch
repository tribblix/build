-static int find_start(jvm_agent_t* J, uint64_t ptr, uint64_t *startp) {
-  int err;
-  int i;
-
-  for (i = 0; i < J->Number_of_heaps; ++i) {
-    *startp = 0;
-    if (codeheap_contains(i, J, ptr)) {
-      int32_t used;
